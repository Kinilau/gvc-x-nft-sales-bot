modules = ["python-3.11"]
[agent]
expertMode = true

[nix]
channel = "stable-25_05"
packages = ["freetype", "gmp", "lcms2", "libimagequant", "libjpeg", "libtiff", "libwebp", "libxcrypt", "openjpeg", "tcl", "tk", "zlib"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "nft-sales-bot"

[[workflows.workflow]]
name = "nft-sales-bot"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python nft_sales_bot.py"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 32967
externalPort = 3000

[[ports]]
localPort = 34345
externalPort = 5173

[[ports]]
localPort = 35427
externalPort = 3003

[[ports]]
localPort = 36779
externalPort = 5000

[[ports]]
localPort = 42489
externalPort = 4200

[[ports]]
localPort = 44167
externalPort = 3002

[[ports]]
localPort = 44975
externalPort = 3001

[deployment]
deploymentTarget = "vm"
run = ["gunicorn", "--bind", "0.0.0.0:5000", "--reuse-port", "-w", "4", "nft_sales_bot:app"]
